{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.ibs.ibsappidealsalerorderreport",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.2",
      "toolsId": "d3fe17b9-99fb-480b-bebb-6c0f1aebab33"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/ideal-purchase-creation-srv/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "SalesOrderReport-display": {
          "semanticObject": "SalesOrderReport",
          "action": "display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.126.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ibs.ibsappidealsalerorderreport.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.ibs.ibsappidealsalerorderreport.view",
        "controlAggregation": "beginColumnPages",
        "controlId": "layout",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMasterPage",
          "pattern": "",
          "target": [
            "TargetMasterPage"
          ]
        },
        {
          "name": "detailPage",
          "pattern": "detailPage/{PR_NO}",
          "target": [
            "TargetMasterPage",
            "detailPage"
          ]
        }
      ],
      "targets": {
        "TargetMasterPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "MasterPage",
          "controlAggregation": "beginColumnPages",
          "viewName": "MasterPage"
        },
        "detailPage": {
          "viewType": "XML",
          "viewName": "detailPage",
          "controlAggregation": "midColumnPages",
          "viewId": "detailPage"
        }
      }
    },
    "rootView": {
      "viewName": "com.ibs.ibsappidealsalerorderreport.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "ibs_bs_ideal_ps"
  }
}
